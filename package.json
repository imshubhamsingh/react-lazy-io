{
  "name": "react-lazy-io",
  "version": "0.0.0",
  "description": "react lazyload using using IntersectionObserver API",
  "module": "build/index.esm.js",
  "main": "build/index.js",
  "author": "Shubham Singh",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "postinstall": "husky install",
    "local:publish": "yalc publish",
    "prepublishOnly": "yarn run build && pinst --disable",
    "postpublish": "pinst --enable",
    "tsc": "tsc",
    "clean": "rm -rf build",
    "build": "rollup --c ./rollup.config.js",
    "build:debug": "rollup -c ./rollup.config.js --config-debug -w"
  },
  "export": {
    ".": {
      "require": "./build/index.js",
      "import": "./build/index.esm.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imshubhamsingh/react-lazy-io.git"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-private-methods": "^7.16.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/react": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "rollup": "^2.60.0",
    "typescript": "^4.4.4",
    "yalc": "1.0.0-pre.50"
  },
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "engines": {
    "node": ">=14.17.0",
    "yarn": ">=1.16.0"
  }
}
